<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.therussells.musicstructureapp4">

    <application
        android:allowBackup="true"
        android:fullBackupContent="@xml/backup_descriptor"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".GenreActivity"
            android:label="@string/genre"/>
        <activity android:name=".ArtistActivity"
            android:label="@string/artist"/>
        <activity android:name=".AlbumActivity"
            android:label="@string/album"/>
        <activity android:name=".PlaylistActivity"
            android:label="@string/playlist"/>
        <activity android:name=".FavoriteSongsActivity"
            android:label="Favorites"/>
    </application>

</manifest>